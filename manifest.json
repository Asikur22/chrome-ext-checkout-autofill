{
	"manifest_version" : 3,
	"name" : "Checkout Autofill",
	"version" : "1.5",
	"description" : "Auto-fills WordPress checkout forms with fake data.",
	"permissions" : [
		"activeTab",
		"scripting",
		"storage"
	],
	"action" : {
		"default_popup" : "index.html",
		"default_icon" : "assets/icon.png"
	}
}
